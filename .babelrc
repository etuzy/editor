{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-transform-spread"],
    ["module-resolver", {
      "alias": {
        "@": "./src",
        "@editor": "./src/editor",
        "@components": "./src/components",
        "@config": "./src/config",
        "@utils": "./src/utils",
      }
    }]
  ]
}
